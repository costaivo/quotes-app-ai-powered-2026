#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/projects/2025/3_apps/quotes-app-ai-powered-2025/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.3.1_typescript@5.9.2/node_modules/@commitlint/cli/node_modules:/mnt/d/projects/2025/3_apps/quotes-app-ai-powered-2025/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.3.1_typescript@5.9.2/node_modules/@commitlint/node_modules:/mnt/d/projects/2025/3_apps/quotes-app-ai-powered-2025/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.3.1_typescript@5.9.2/node_modules:/mnt/d/projects/2025/3_apps/quotes-app-ai-powered-2025/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/projects/2025/3_apps/quotes-app-ai-powered-2025/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.3.1_typescript@5.9.2/node_modules/@commitlint/cli/node_modules:/mnt/d/projects/2025/3_apps/quotes-app-ai-powered-2025/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.3.1_typescript@5.9.2/node_modules/@commitlint/node_modules:/mnt/d/projects/2025/3_apps/quotes-app-ai-powered-2025/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.3.1_typescript@5.9.2/node_modules:/mnt/d/projects/2025/3_apps/quotes-app-ai-powered-2025/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@commitlint/cli/cli.js" "$@"
else
  exec node  "$basedir/../@commitlint/cli/cli.js" "$@"
fi
